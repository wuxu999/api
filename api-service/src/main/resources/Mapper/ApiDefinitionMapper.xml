        <result column="update_time" property="updateTime" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, group_id, name, request_type, message_type, request_url, remark, create_time, update_time, is_deleted
    </sql>

</mapper>
